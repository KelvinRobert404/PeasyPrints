<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows10.0.19041.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>PeasyPrint.Helper</AssemblyName>
    <RootNamespace>PeasyPrint.Helper</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="PdfiumViewer.Core" Version="2.16.0" />
    <PackageReference Include="PdfiumViewer.Native.x64" Version="2023.10.21.0" />
    <PackageReference Include="System.Drawing.Common" Version="8.0.7" />
  </ItemGroup>
</Project>

